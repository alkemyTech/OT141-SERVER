{
  "name": "ong-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "dev": "cross-env NODE_ENV=development nodemon app.js",
    "test:db:migrate": "cross-env NODE_ENV=test npx sequelize-cli db:migrate",
    "test:db:seed": "cross-env NODE_ENV=test npx sequelize-cli db:seed:all",
    "test": "cross-env NODE_ENV=test jest --verbose --silent",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.6.1",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "express-validator": "^6.14.0",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "mysql2": "^2.2.3",
    "sequelize": "^6.3.5"
  },
  "devDependencies": {
    "nodemon": "^2.0.15",
    "jest": "^27.5.1",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.2.2"
  }
}
